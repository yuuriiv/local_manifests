		clone-depth="1"/>

	<project
		path="vendor/xiaomi/sm6225-common"
		name="vendor_xiaomi_sm6225-common"
		remote="pixel-devices-blobs"
		clone-depth="1"/>

	<project
		path="hardware/xiaomi"
