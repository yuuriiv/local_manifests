<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android">

	<remote
		name="mobxv"
		fetch="https://github.com/mobxv"
		revision="thirteen"/>

	<remote
		name="pixel"
		fetch="https://github.com/PixelExperience"
		revision="thirteen"
		review="gerrit.pixelexperience.org"/>

	<remote
		name="pixel-devices"
		fetch="https://github.com/PixelExperience-Devices"
		revision="thirteen"/>

	<remote
		name="pixel-devices-blobs"
		fetch="https://gitlab.pixelexperience.org/android/vendor-blobs"
		revision="thirteen"/>

	<project
		path="device/xiaomi/spes"
		name="device_xiaomi_spes"
		remote="pixel-devices"
		clone-depth="1"/>

	<project
		path="device/xiaomi/spes-kernel"
		name="device_xiaomi_spes-kernel"
		remote="pixel-devices"
		clone-depth="1"/>

	<project
		path="device/xiaomi/sm6225-common"
