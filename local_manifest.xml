		clone-depth="1"/>

	<project
		path="device/qcom/wlan"
		name="device_qcom_wlan"
		remote="pixel-devices"
		clone-depth="1"/>

	<project
		path="vendor/qcom/common"
		name="vendor_qcom_common"
		remote="pixel-devices-blobs"
		clone-depth="1"/>

	<project
		path="vendor/qcom/opensource/commonsys-intf/bluetooth"
		name="vendor_qcom_opensource_commonsys-intf_bluetooth"
		remote="pixel"
		clone-depth="1"/>

</manifest>

