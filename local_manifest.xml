		clone-depth="1"/>

	<project
		path="kernel/xiaomi/sm6225"
		name="kernel_xiaomi_sm6225"
		remote="pixel-devices"
		clone-depth="1"/>

	<project
		path="vendor/xiaomi/spes"
		name="vendor_xiaomi_spes"
